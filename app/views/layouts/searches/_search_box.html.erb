<% if admin_signed_in? %>
  <%= form_with url: admin_search_path, local: true, method: :get, class: "my-1" do |f| %>
    <%= render "layouts/searches/search_form", f: f %>
  <% end %>
<% elsif user_signed_in? %>
  <%= form_with url: search_path, local: true, method: :get, class: "my-1" do |f| %>
    <%= render "layouts/searches/search_form", f: f %>
  <% end %>
<% end %>
